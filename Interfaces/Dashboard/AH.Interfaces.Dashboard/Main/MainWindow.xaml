<Window x:Class="AH.Interfaces.Dashboard.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AH.Interfaces.Dashboard.Main"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1024" WindowStartupLocation="CenterScreen">
    <Grid>
        <TabControl Margin="5">
            <TabItem Header="Home">
                <Frame x:Name="frHome" NavigationUIVisibility="Hidden"></Frame>
            </TabItem>
            <TabItem Header="Discovery">
                <Frame Source="/Main/Discovery/DiscoveryPage.xaml"></Frame>
            </TabItem>
            <TabItem Header="Configuration">
                <Frame Source="/Main/Configuration/ConfigurationPage.xaml"></Frame>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
